<div class="signin_form">
<div class="signin_form_detail">

<h2>WELCOME</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.email_field :email, autofocus: true, autocomplete: "email",placeholder:"Email",class: "new_session_btn" %>
  </div>

  <div class="field">
    <%= f.password_field :password, autocomplete: "current-password",placeholder:"Password",class: "new_session_btn" %>
  </div>


    <%= f.submit "SIGNIN" , class: "new_submit_btn" %>

<% end %>


</div>
</div>