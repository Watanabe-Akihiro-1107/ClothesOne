<div class="item_index_main clearfix">
	<%= render "user/items/sidebar" %>
	<% if flash[:notice] %>
		<div class="flash">
			<p class='flash_message'><%= flash[:notice]%></p>
		</div>
	<% end %>
	<%= render partial: 'user/items/item_index', locals: { items: @items} %>
	<% if params[:brand_name]%>
		<%= render "user/items/googlemaps"%>
	<% end %>
</div>




